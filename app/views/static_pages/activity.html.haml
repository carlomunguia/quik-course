- content_for :title do
  QuikCourse
  \|
  Activity

%h3
  .text-center
    Activity
    .badge.bg-info
      = @activities.count
%p
%ul.list-group
  - @activities.each do |activity|
    .row
    %li.list-group-item
      = activity.id
      = activity.trackable_type
      = activity.trackable_id
      = link_to activity.trackable, activity.trackable
      = activity.key
      = activity.parameters
      = activity.owner
